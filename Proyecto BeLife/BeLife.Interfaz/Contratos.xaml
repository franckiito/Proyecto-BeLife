<Window x:Class="BeLife.Interfaz.Contratos"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BeLife.Interfaz"
        mc:Ignorable="d"
        Title="Seguros BeLife - Contratos" Height="503" Width="835" ResizeMode="NoResize">

    <Window.Resources>

        <DataTemplate x:Key="PlanTemplate">
            <StackPanel Orientation="Horizontal">
                <TextBlock Text="{Binding Nombre}"/>
            </StackPanel>
        </DataTemplate>
    </Window.Resources>
    <Grid>
        <Image HorizontalAlignment="Left" Height="68" Margin="654,29,0,0" VerticalAlignment="Top" Width="129" Source="Imagenes/Logo BeLife.png" Stretch="Fill"/>
        <GroupBox Header="Contrato" HorizontalAlignment="Left" Height="288" Margin="10,119,0,0" VerticalAlignment="Top" Width="809">
            <Label Content="Fecha Creacion" HorizontalAlignment="Left" Margin="10,20,0,0" VerticalAlignment="Top"/>
        </GroupBox>
        <DatePicker HorizontalAlignment="Left" Margin="134,158,0,0" VerticalAlignment="Top"/>


    </Grid>
</Window>
